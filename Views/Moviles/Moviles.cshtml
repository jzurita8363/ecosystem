@using ecosystem.Models

@{
    ViewBag.Title = "Moviles";
    Layout = "~/mainPage.cshtml";
}
    <div>
        <div class="row" style="padding-right: 5px;margin-right: 0px;margin-left: 0px;">
            <div class="col-md-8">
                <label id="#empresaconf" style="color: white;">@ViewBag.ConductoresConf</label>
                <label id="#perfil" style="color: white;">@ViewBag.Perfil</label>
                <label id="#correoUsuario" style="color: white;">@ViewBag.Correo</label>
            </div>
            <div align="right" class="col-md-4" style="padding: 10px 5px 0px 0px; font-size: 0.93rem;">
                <label id="#ruta">Bienvenido(a)</label>
                <label id="#usuario" style="font-weight: bold;">@ViewBag.Usuario</label>
                <button id="salirUsuario" data-toggle="tooltip" data-placement="bottom" title="Cerrar Sesión" class="btn" style="color: #009900; font-size: 25px; padding-left: 0px; padding-right: 0px; padding-bottom: 10px;"><i class="fas fa-sign-out-alt"></i></button>
            </div>
        </div>
    </div>

<div class="row animated slideInRight" style="margin-top: 15px; margin-left:5px; margin-right: 5px; background-color: #f8f9fa ; color: white; padding-left: 5px; font-weight: bold; font-size: 1.1rem; font-style: italic; text-align: center;">
    <div class="col-md-3" style="background-color: #28a745; padding-top: 10px; padding-right: 0px;">
        INGRESO DE MÓVILES
    </div>
    <div class="col-md-1 triangle-bottomleft">
    </div>
    <div class="col-md-8">
    </div>
</div>

<div id="modalCambioEstado" class="modal fade modal-open animated" tabindex="-1" data-focus-on="input:first" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog modal-lg modal-dialog-centered">
        <div class="modal-content" style="padding: 15px;">
            <div class="card" style="margin-bottom: 5px;">
                <div class="card-header">
                    <a class="card-link" data-toggle="collapse">
                        <p class="font-weight-bold">INDIQUE UN MOTIVO</p>
                    </a>
                </div>
                <div class="card-body">
                    <div class="row">
                        <label for="">Detalle el motivo por el cual está realizando el cambio de Estado</label>
                        <div class="input-group">
                            <textarea id="motivoCambioEstado" type="text" rows="3" class="form-control" autocomplete="off"></textarea>
                        </div>
                    </div>
                    <div class="row" style="margin-top: 15px; margin-left:15px; margin-right: 15px;">
                        <button id="guardarCambioEstado" type="button" class="btn btn-info" style="margin-right: 5px; font-size: 0.85rem;">Guardar</button>
                        <button id="cerrarCambioEstador" type="button" class="btn btn-light" style="margin-right: 5px; font-size: 0.85rem;">Cancelar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!--Modal Cambio de Estado-->
<div id="desvincularConductor" class="modal fade modal-open animated" tabindex="-1" data-focus-on="input:first" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog modal-lg modal-dialog-centered">
        <div class="modal-content" style="padding: 15px;">
            <div class="card" style="margin-bottom: 5px;">
                <div class="card-header">
                    <a class="card-link" data-toggle="collapse">
                        <p class="font-weight-bold">INDIQUE UN MOTIVO</p>
                    </a>
                </div>
                <div class="card-body">
                    <div class="form-group">
                        <label for="validationDefault01" class=" col-form-label">Agregar a BlackList</label>
                        <fieldset id="agregarBL">
                            <div class="custom-control custom-radio custom-control-inline">
                                <input type="radio" class="custom-control-input" id="siBL" name="example" checked="checked" value="SI">
                                <label class="custom-control-label" for="siBL">SI</label>
                            </div>
                            <div class="custom-control custom-radio custom-control-inline">
                                <input type="radio" class="custom-control-input" id="noBL" name="example" value="NO">
                                <label class="custom-control-label" for="noBL">NO</label>
                            </div>
                        </fieldset>
                    </div>
                    <div class="row">
                        <label for="">Detalle el motivo por el cual está desvinculando al conductor</label>
                        <div class="input-group">
                            <textarea id="motivoDesvincular" type="text" rows="3" class="form-control" autocomplete="off"></textarea>
                        </div>
                    </div>
                    <div class="row" style="margin-top: 15px; margin-left:15px; margin-right: 15px;">
                        <button id="guardarMotivoDesvincular" type="button" class="btn btn-info" style="margin-right: 5px; font-size: 0.85rem;">Guardar</button>
                        <button id="cerrarMotivoDesvincular" type="button" class="btn btn-light" data-dismiss="modal" style="margin-right: 5px; font-size: 0.85rem;">Cerrar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!--Modal Cambio de Estado-->
<div id="cambiarEstado" class="modal fade modal-open animated" tabindex="-1" data-focus-on="input:first" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog modal-lg modal-dialog-centered">
        <div class="modal-content" style="padding: 15px;">
            <div class="card" style="margin-bottom: 5px;">
                <div class="card-header">
                    <a class="card-link" data-toggle="collapse">
                        <p class="font-weight-bold">CAMBIAR ESTADO</p>
                    </a>
                </div>
                <div class="card-body">
                    <div class="row" id="filaEstado">
                        <label style="margin-right: 5px; margin-left: 14px; font-weight: bold">Estado del Vehículo</label>
                    </div>
                    <div class="row">
                        <div class="container" style="padding-top: 5px;">
                            <table id="tbl-usuariosMovil" class="table table-striped table-bordered customtable" cellspacing="0" style="width:100%;">
                                <thead>
                                    <tr>
                                        <th>RUT</th>
                                        <th>Nombre y Apellido</th>
                                        <th>Perfil</th>
                                        <th>Estado</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                    <div class="row" style="margin-top: 15px; margin-left:15px; margin-right: 15px;">
                        <button id="guardarEstados" type="button" class="btn btn-info" data-dismiss="modal" style="margin-right: 5px; font-size: 0.85rem;">Aceptar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!--UN MOVIL-->
<div id="listaMoviles" class="modal fade modal-open animated" tabindex="-1" data-focus-on="input:first" data-keyboard="false" data-backdrop="static">
